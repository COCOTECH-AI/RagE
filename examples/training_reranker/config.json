{
    "name": "ranker_phobert", 
    "tokenizer_name": "vinai/phobert-base-v2", 
    "architectures": {
        "model_name": "vinai/phobert-base-v2",
        "type_backbone": "bert", 
        "dropout": 0.1, 
        "using_hidden_states": false, 
        "pooling": "dense_first", 
        "num_label": 1,
        "required_grad": true
    },
    "loss": "binary_cross_entropy",
    "max_length": 256,
    "batch_size": 512,
    "shuffle": true, 
    "num_workers": 16, 
    "pin_memory": true, 
    "prefetch_factor": 8, 
    "persistent_workers": true, 
    "gradient_accumlation_steps": 8, 
    "learning_rate": 2e-4, 
    "weight_decay": 0.1, 
    "eps": 1e-6, 
    "warmup_steps": 150, 
    "epochs": 6, 
    "path_ckpt_step": "step_ranker_phobert.pt", 
    "verbose": 5, 
    "step_save": 100, 
    "path_ckpt_epoch": "best_ranker_phobert.pt", 
    "path_train": "small_msmacro_ranker.json",
    "path_eval": null, 
    "device": "cuda"
}