{
    "name": "embedding_zalo_phobert", 
    "tokenizer_name": "vinai/phobert-base-v2", 
    "architectures": {
        "model_name": "vinai/phobert-base-v2",
        "type_backbone": "bert", 
        "concat_embeddings": true,
        "dropout": 0.1, 
        "using_hidden_states": true, 
        "hidden_dim": 768,
        "num_label": 1,
        "required_grad": true
    },
    "loss": "binary_cross_entropy",
    "max_length": 256,
    "batch_size": 32,
    "shuffle": true, 
    "num_workers": 16, 
    "pin_memory": true, 
    "prefetch_factor": 8, 
    "persistent_workers": true, 
    "gradient_accumlation_steps": 8, 
    "learning_rate": 1e-4, 
    "weight_decay": 0.1, 
    "eps": 1e-6, 
    "warmup_steps": 150, 
    "epochs": 10, 
    "path_ckpt_step": "step_ckpt_embedding_zalo_phobert.pt", 
    "verbose": 1, 
    "step_save": 100, 
    "path_ckpt_epoch": "best_ckpt_embedding_zalo_phobert.pt", 
    "path_train": "zalo_train.json",
    "path_eval": null, 
    "device": "cuda"
}